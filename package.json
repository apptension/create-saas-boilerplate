{
  "name": "create-saas-boilerplate",
  "version": "0.0.7",
  "description": "Create new SaaS Boilerplate instance CLI tool",
  "author": "Apptension sp. z o.o.",
  "bin": {
    "create-saas-boilerplate": "./bin/run"
  },
  "homepage": "https://github.com/apptension/create-saas-boilerplate",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "apptension/create-saas-boilerplate",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-version": "^1.3.4",
    "@types/fs-extra": "^11.0.1",
    "fs-extra": "^11.1.1",
    "listr2": "^5.0.8",
    "node-fetch": "2",
    "oclif": "^3",
    "simple-git": "^3.18.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.22",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.27",
    "@types/node-fetch": "2.x",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@types/which": "^2.0.2",
    "chai": "^4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "mocha": "^9",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "^15.0.2",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "create-saas-boilerplate",
    "dirname": "create-saas-boilerplate",
    "commands": "./dist/commands",
    "default": "init",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "topicSeparator": " ",
    "topics": {
      "init": {
        "description": "Initialize new project"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "bugs": "https://github.com/apptension/create-saas-boilerplate/issues",
  "keywords": [
    "saas",
    "boilerplate",
    "startup",
    "apptension"
  ],
  "types": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged --pattern 'src/**/*.(ts|tsx)'"
    }
  }
}
